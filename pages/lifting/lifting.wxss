.container{
  padding:0 30rpx;
  border-top:1px solid #E6E6E6;
}
page{
  background: #fff
}
.block{
  margin-top:30rpx;
  width:100%;
  height: 200rpx;
  border-radius:10rpx;
  background-color: #ffffff;
  box-shadow: 0 2rpx 8px 2rpx rgba(198, 201, 208, 0.5);
  display: flex;
  align-items: center;
  position: relative;
  /* justify-content: space-between; */
}
.blockOn{
  border-bottom: 8rpx solid #36d3d5;
}
.selectAddress{
  border-bottom: 6rpx solid #36d3d5;  
}
.block view{
  
}
.block .left image{
  width:57rpx;
  height:57rpx;
  margin:0 30rpx;
}
.middle .title > text{
  font-size:30rpx;
  color:#333;
  font-weight:600;

}
.middle .title .phone{
  margin-left:30rpx;
}
.middle .address{
  margin-top:26rpx;
  font-size:26rpx;
  color:#666;
}
.right{
  position: absolute;
  right:30rpx;
}
.right image{
  width:34rpx;
  height: 34rpx;
}

.addBox{
    margin-top:30rpx;
    width:100%;
    height: 180rpx;
    border-radius:10rpx;
    background-color: #ffffff;
    box-shadow: 0 2rpx 8px 2rpx rgba(198, 201, 208, 0.5);
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}
.addBox .add{
  display: flex;
}
.addBox .add image{
  width:50rpx;
  height: 50rpx;
  margin-right:30rpx;
}
.addBox .add text{
  font-weight: 600;
  font-size:30rpx;
  color:#111;
}
.container button{
  margin-top:30rpx;
  background:#36D3D5;
  border-radius:50rpx;
  color:#fff;
  font-size: 32rpx;
  line-height: 94rpx;
  height:94rpx;
}
.lifting{
  text-align: center;
  padding-top: 20px;
}
.lifting text:nth-child(1){
  font-size: 30rpx;
  font-weight: 600
}
.lifting text:nth-child(2){
  font-size: 30rpx;
  font-weight: 600;
  color: #36D3D5
}
.right text{
  color: #999
}
.cu-form-group{
  padding: 0;
  display: block
}
.checkedbox{
  width: 15%;
  display: flex;
  justify-content: center;
  align-items: center
}