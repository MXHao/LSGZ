/* pages/LogisticsDetails/LogisticsDetails.wxss */
page{
  background: #fff
}
.contentNew{
  padding: 30rpx
}
.Logistics{
  background: #fff;
  padding:0 30rpx;
  box-shadow: 0rpx 0rpx 20rpx #e5e5e5;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  position: sticky;
  top:0;
  width: 100%;
  z-index: 99
}
.Logisticsx{
  background: #fff;
  padding:0 30rpx;
  box-shadow: 0rpx 0rpx 20rpx #e5e5e5;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}
.LogisticsList{
  border-bottom: 1rpx solid #e5e5e5;
  padding: 30rpx 0
}
.LogisticsName{
  font-weight: 600;
  font-size: 30rpx
}
.LogisticsText{
  color: #999
}
.LogisticsListBottom{
  padding: 30rpx 0;
  display: flex;
  justify-content: space-between;
  align-items:center
}
.LogisticsRight text{
  border:1rpx solid #36d3d5;
  color: #36d3d5;
  border-radius: 30rpx;
  padding: 5rpx 20rpx
}
.LogisticsTitle{
  font-size: 30rpx;
  font-weight: 600;
  line-height: 35rpx
}
.LogisticsTime{
  color: #fff
}